{
    "name": "igloo-estate-backend",
    "version": "1.0.0",
    "description": "Backend for Igloo Estate - Student housing platform with role-based access control",
    "main": "server.js",
    "scripts": {
        "dev": "nodemon server.js",
        "start": "node server.js",
        "db:generate": "npx prisma generate",
        "db:migrate": "npx prisma migrate dev",
