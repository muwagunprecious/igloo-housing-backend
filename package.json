        "db:studio": "npx prisma studio",
        "db:seed": "node seed_user.js",
        "seed:all": "node seed-all.js",
        "postinstall": "npx prisma generate"
    },
    "keywords": [
        "real-estate",
        "student-housing",
        "nodejs",
        "express",
        "prisma",
        "socket.io"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.7.0",
        "axios": "^1.13.5",
        "bcryptjs": "^3.0.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "form-data": "^4.0.5",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.18.0",
        "socket.io": "^4.6.0"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "prisma": "^5.7.0"
    }
